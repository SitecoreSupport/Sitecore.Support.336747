      <pipelines>
          <group groupName="WebDAV" name="WebDAV">
              <pipelines>
                  <supportWebDAVUrl>
                      <processor type="Sitecore.Support.Pipelines.WebDAV.Processors.DefaultSupportWebDAVUrlResolver, Sitecore.Support.336747" patch:instead="*[@type='Sitecore.Pipelines.WebDAV.Processors.DefaultSupportWebDAVUrlResolver, Sitecore.Kernel']" />
